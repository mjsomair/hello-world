Jenkinsfile (Declarative Pipeline)
pipeline {
    agent any
    stages {
        stage('Example Build') {
            steps {
                echo("hello build")
            }
        }
        stage('Example Test') {
             steps {
                echo("hello Test")
            }
 	 stage('Example Deploy') {
             steps {
                echo("hello deploy")
            }
        }
    }
}
